  <% posts.forEach(function(post){ %>
    <div class="box">
        <img src="/images/uploads/<%= post.image %>" alt="<%= post.title %>" class="src">
        <div class="caption" style="display: flex;">
            <% const username = post.user.username %>
           
            <div class="caption" style="margin-right: 5px;"><a style="text-decoration: none; color: aquamarine;" href="usersprofile/<%= username %>" >@<%= username %> </a></div>
            <div class="caption" style="text-align: center;"><%= post.imageText %>  </div>
        </div>
       
    </div>

  <% }) %>

   
     
